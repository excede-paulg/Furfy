{
	"name": "@pinegrow/tailwindcss-plugin",
	"version": "3.0.35",
	"description": "Pinegrow TailwindCSS Plugin",
	"type": "module",
	"files": [
		"dist",
		"./types.d.ts"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.cjs",
	"types": "./types.d.ts",
	"exports": {
		".": {
			"types": "./types.d.ts",
			"import": "./dist/index.cjs",
			"require": "./dist/index.cjs"
		},
		"./dev": {
			"import": "./src/index.js",
			"require": "./src/index.js"
		}
	},
	"bin": {
		"tailwindcss-plugin": "./dist/cli.cjs"
	},
	"keywords": [
		"pinegrow",
		"vue designer",
		"vuedesigner",
		"vue designer",
		"tailwindcss-plugin",
		"@pinegrow/tailwindcss-plugin"
	],
	"author": "Pinegrow (http://pinegrow.com/)",
	"license": "MIT",
	"scripts": {
		"build:tailwindcss-plugin": "shx chmod +x ./src/cli.cjs && webpack -- --env mode=production tailwindcss",
		"build:tailwindcss-plugin:dev": "shx chmod +x ./src/cli.cjs && webpack -- --env mode=development tailwindcss",
		"publish-beta": "npm run increment-beta-version && npm publish --tag beta",
		"increment-beta-version": "npm version prerelease --preid=beta",
		"publish-patch": "npm run increment-version && npm publish",
		"increment-version": "npm version patch"
	},
	"dependencies": {
		"arg": "^5.0.2",
		"chroma-js": "^2.4.2",
		"jiti": "^1.17.2",
		"postcss": "^8.3.11",
		"postcss-selector-parser": "^6.0.11",
		"sucrase": "^3.29.0"
	},
	"peerDependencies": {
		"tailwindcss": ">=3"
	}
}
